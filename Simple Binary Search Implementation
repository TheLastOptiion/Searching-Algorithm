int binary_search(int arr[],int arr_size,int find){
    
    int lo=0,hi=arr_size-1,mid;
    
    while(lo-hi!=1){
        mid = (hi+lo+1)/2;
        if(fnd<arr[mid]){
            hi=mid;
        }
        else if(find>arr[mid]){
            lo=mid+1;
        }
        else if(find==arr[mid])
            return mid;
     }
    return -1;
}
